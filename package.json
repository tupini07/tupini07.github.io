  "version": "0.8.0",
  "name": "gatsby-wiki-blog",
  "description": "Reference page using gatsby, providing a blog and wiki functionality.",
  "main": "n/a",
  "repository": "git+https://github.com/tupini07/tupini07.github.io.git",
  "keywords": [
    "gatsby",
    "blog",
    "wiki",
    "react"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "clean:public": "rm -rf public",
    "clean": "npm run clean:public",
    "lint": "./node_modules/.bin/eslint --cache --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write './*.js' 'src/**/*.js' 'src/**/*.jsx'",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy-ci": "gatsby build && gh-pages -d public -b master -r https://$GH_TOKEN@github.com/tupini07/tupini07.github.io.git",
    "codegen": "graphql-codegen --config graphql-codegen.yml"
  },
